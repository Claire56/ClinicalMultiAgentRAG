# Database
DATABASE_URL=postgresql+asyncpg://healthcare_user:healthcare_pass@localhost:5432/healthcare_db
SYNC_DATABASE_URL=postgresql://healthcare_user:healthcare_pass@localhost:5432/healthcare_db

# Redis
REDIS_URL=redis://localhost:6379/0

# Security
SECRET_KEY=your-secret-key-change-in-production-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# API Keys
API_KEY_HEADER_NAME=X-API-Key

# AI Providers
OPENAI_API_KEY=your-openai-api-key-here
ANTHROPIC_API_KEY=your-anthropic-api-key-here
LLM_PROVIDER=openai

# Pinecone
PINECONE_API_KEY=your-pinecone-api-key-here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=healthcare-knowledge-base

# LangSmith
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=healthcare-rag
LANGSMITH_TRACING=true

# Tavily (Real-time Web Search)
TAVILY_API_KEY=your-tavily-api-key-here
TAVILY_ENABLED=true
TAVILY_MAX_RESULTS=5

# Application
APP_NAME=Healthcare Agentic RAG API
APP_VERSION=1.0.0
DEBUG=False
ENVIRONMENT=production

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

